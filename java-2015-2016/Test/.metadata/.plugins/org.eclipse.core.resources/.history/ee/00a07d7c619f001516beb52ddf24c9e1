package shapes;

public class Square implements Shape {

	private int side;
	
	public float calculateSurface() {
		int surface = side *side;
		return surface;
	}

	
	public float calculatePerimetre() {
		float perimetre = 4*side;
		return perimetre;
	}


	public Square(int side) {
		super();
		this.side = side;
	}

}
