package shapes;

public class Triangle implements Shape {

	private float sideA;
	private float sideB;
	private float sideC;
	private float hight;
	
	public float calculateSurface() {
		
		return 0;
	}

	
	public float calculatePerimetre() {
		
		return 0;
	}


	public Triangle(float sideA, float sideB, float sideC, float hight) {
		super();
		this.sideA = sideA;
		this.sideB = sideB;
		this.sideC = sideC;
		this.hight = hight;
	}

}
